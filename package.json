{
  "name": "react-app-demo",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "start": "parcel ./src/index.html",
    "watch": "parcel watch src/index.html --public-url ./",
    "dev": "npm run build:develop",
    "prod": "npm run build:release",
    "build:develop": "parcel build src/index.html --public-url ./ --out-dir dist --no-minify --no-cache",
    "build:release": "parcel build src/index.html --public-url ./ --out-dir dist",
    "test": "jest --watch",
    "audit": "npm audit --registry='https://registry.npmjs.org/'"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/setupTests.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,md,css,html}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.9.8",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.6",
    "connected-react-router": "^6.8.0",
    "immutability-helper": "^3.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "typesafe-actions": "^5.1.0",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/node": "13.9.5",
    "@types/react": "16.9.27",
    "@types/react-dom": "16.9.5",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "remote-redux-devtools": "^0.5.16",
    "@types/prettier": "1.19.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "prettier": "2.0.2",
    "parcel-bundler": "1.12.4"
  }
}
