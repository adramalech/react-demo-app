{
  "name": "react-app-demo",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "last 4 version",
    "> 2%",
    "not dead",
    "IE 11",
    "not op_mob 59"
  ],
  "scripts": {
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "lint:verbose": "tsc && eslint '*/**/*.{js,ts,tsx}' --fix",
    "prettier-format": "prettier --config .prettierrc '*/**/*.{js,ts,tsx}' --write",
    "start": "parcel ./src/index.html",
    "watch": "parcel watch src/index.html --public-url ./",
    "dev": "npm run build:develop",
    "prod": "npm run build:release",
    "build:develop": "parcel build src/index.html --public-url ./ --out-dir dist --no-minify --no-cache",
    "build:release": "parcel build src/index.html --public-url ./ --out-dir dist",
    "test": "jest --watch",
    "audit": "npm audit --registry='https://registry.npmjs.org/'"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/setupTests.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,md,css,html}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "connected-react-router": "^6.8.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "immer": "^7.0.9",
    "typesafe-actions": "^5.1.0",
    "typescript": "3.9.7"
  },
  "devDependencies": {
    "@types/jest": "26.0.14",
    "@types/node": "14.11.5",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.5",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-logger": "^3.0.8",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "remote-redux-devtools": "^0.5.16",
    "supertest": "^5.0.0",
    "@types/prettier": "^2.1.1",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "eslint-plugin-react": "7.21.4",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "2.1.2",
    "parcel-bundler": "1.12.4"
  }
}
