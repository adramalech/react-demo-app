{
  "name": "my-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "vitest"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx|cjs|mts|mts)": [
      "prettier --config .prettierrc.js --write",
      "eslint './src/**' --ext .js,.jsx,.ts,.tsx,.mts,.cjs,.mjs -c .eslintrc.mjs --report-unused-disable-directives --fix --quiet"
    ],
    "src/**/*.(json|css|md)": [
      "prettier --config .prettierrc.js --write",
      "eslint './src/**' --ext .json,.css,.md -c .eslintrc.mjs --report-unused-disable-directives --fix --quiet"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "husky": "^9.1.7",
    "immer": "^10.1.1",
    "react": "^19.0.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "redux": "^5.0.1",
    "redux-saga": "^1.3.0"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.17",
    "postcss": "^8.4.49",
    "autoprefixer": "^10.4.20",
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/react-redux": "^7.1.34",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "globals": "^15.14.0",
    "prettier": "^3.3.3",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.18.2",
    "jsdom": "^25.0.1",
    "vitest": "^2.1.8",
    "vite": "^6.0.6"
  }
}